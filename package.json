{
  "name": "pizza-locator",
  "version": "0.0.1",
  "description": "",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dockerBuild": "docker build -t mitwilsch/pizza-locator:latest .",
    "prodClient": "npx http-server dist -p 80",
    "prodServer": "node src/server/index.js",
    "start": "npx concurrently \"npm run prodClient\" \"npm run prodServer\""
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "concurrently": "^5.3.0",
    "pizzapi": "0.0.8",
    "react": "^16.13.1",
    "react-dom": "^17.0.1",
    "typeface-roboto": "^1.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@material-ui/docs": "^4.0.0-beta.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.3",
    "prettier": "^1.19.1",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
